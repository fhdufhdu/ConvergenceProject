            <Font name="LG Smart UI Regular" size="15.0" />
         </font>
      </Text>
      <Text fx:id="t_rsv_rate" layoutY="273.0" strokeType="OUTSIDE" strokeWidth="0.0" text="예매율" textAlignment="CENTER" wrappingWidth="200.0">
